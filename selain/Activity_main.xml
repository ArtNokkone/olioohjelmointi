<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:onClick="refresh"
    tools:context=".MainActivity"
    tools:ignore="WebViewLayout">

    <WebView
        android:id="@+id/selain"
        android:layout_width="419dp"
        android:layout_height="533dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="16dp"
        android:overScrollMode="ifContentScrolls"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.192"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="NotSibling" />

    <EditText
        android:id="@+id/editTextWebAddress"
        android:layout_width="273dp"
        android:layout_height="63dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:ems="10"
        android:inputType="text"
        android:onClick="readText"
        android:text="url"
        app:layout_constraintBottom_toTopOf="@+id/selain"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toEndOf="@+id/button"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button"
        android:layout_width="112dp"
        android:layout_height="55dp"
        android:layout_above="@+id/selain"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_toStartOf="@+id/editTextWebAddress"
        android:layout_toLeftOf="@+id/editTextWebAddress"
        android:onClick="refresh"
        android:text="REFRESH"
        app:layout_constraintBottom_toTopOf="@+id/selain"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button2"
        android:layout_width="93dp"
        android:layout_height="47dp"
        android:layout_marginEnd="204dp"
        android:layout_marginRight="204dp"
        android:onClick="shoutOut"
        android:text="SHOUT"
        app:layout_constraintBottom_toTopOf="@+id/selain"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.748" />

    <Button
        android:id="@+id/button3"
        android:layout_width="80dp"
        android:layout_height="46dp"
        android:layout_marginBottom="20dp"
        android:onClick="initialize"
        android:text="INIT"
        app:layout_constraintBottom_toTopOf="@+id/selain"
        app:layout_constraintEnd_toStartOf="@+id/button5"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button"
        app:layout_constraintVertical_bias="0.622" />

    <Button
        android:id="@+id/button4"
        android:layout_width="74dp"
        android:layout_height="41dp"
        android:onClick="scrollForward"
        android:text="NEXT"
        app:layout_constraintBottom_toTopOf="@+id/selain"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.952"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextWebAddress"
        app:layout_constraintVertical_bias="0.153" />

    <Button
        android:id="@+id/button5"
        android:layout_width="76dp"
        android:layout_height="42dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:onClick="scrollBack"
        android:text="BACK"
        app:layout_constraintBottom_toTopOf="@+id/selain"
        app:layout_constraintEnd_toStartOf="@+id/button4"
        app:layout_constraintTop_toBottomOf="@+id/editTextWebAddress"
        app:layout_constraintVertical_bias="0.142" />

</androidx.constraintlayout.widget.ConstraintLayout>
